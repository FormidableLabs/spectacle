    "react-ga": "^2.4.1",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-router": "^4.2.0",
    "react-router-ga": "^1.0.0",
    "react-side-effect": "^1.1.5",
    "react-static": "^5.9.12",
    "styled-components": "^4.1.1"
