{
  "name": "spectacle",
  "version": "8.4.1",
  "description": "ReactJS Powered Presentation Framework",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start:js": "webpack-dev-server --port=3000 --hot --config examples/js/webpack.config.js",
    "start:md": "webpack-dev-server --port=3100 --hot --config examples/md/webpack.config.js",
    "start:one-page": "open examples/one-page.html",
    "check": "yarn lint && yarn prettier-check && yarn test && yarn typecheck",
    "check-ci": "yarn run check",
    "format": "yarn run lint-fix && yarn run prettier-fix",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js examples scripts src",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier \"**/*.{js,json,ts,css,md}\"",
    "prettier-check": "yarn prettier --check",
    "prettier-fix": "yarn prettier --write",
    "test": "jest",
    "clean": "rimraf dist lib es \"examples/*/dist\"",
    "preversion": "yarn run check",
    "version": "yarn clean && yarn build",
    "build-babel": "babel src --extensions .tsx,.ts,.jsx,.js --ignore \"/__snapshots__/,**/*.test.*\"",
    "build-babel-esm": "builder run build-babel -- -d es",
    "build-babel-cjs": "builder run --env \"{\\\"BABEL_ENV\\\":\\\"cjs\\\"}\" build-babel -- -d lib",
    "build-webpack-base": "webpack",
    "build-webpack-lib-dev": "builder run build-webpack-base -- --config webpack.config.dev.js",
    "build-webpack-lib": "builder run build-webpack-base",
    "build-webpack-examples-js": "builder run build-webpack-base -- --config examples/js/webpack.config.js",
    "build-webpack-examples-md": "builder run build-webpack-base -- --config examples/md/webpack.config.js",
    "build-one-page": "node scripts/one-page.js",
    "build": "builder concurrent --buffer build-babel-esm build-babel-cjs build-webpack-lib build-webpack-lib-dev build-types",
    "build-examples": "builder concurrent --buffer build-webpack-examples-js build-webpack-examples-md",
    "build-types": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit"
  },
  "author": "Formidable Labs <hello@formidable.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle.git"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "broadcast-channel": "^3.2.0",
    "broadcastchannel-polyfill": "^1.0.0",
    "dedent": "^0.7.0",
    "history": "^4.9.0",
    "mdast-builder": "^1.1.1",
    "mdast-util-assert": "^3.0.0",
    "mdast-zone": "^4.0.0",
    "merge-anything": "^3.0.3",
    "mousetrap": "^1.6.5",
    "prism-react-renderer": "^0.1.7",
    "query-string": "^6.8.2",
    "react-fast-compare": "^3.2.0",
    "react-is": "^17.0.2",
    "react-spring": "^8.0.25",
    "react-swipeable": "^6.1.0",
    "react-syntax-highlighter": "^12.2.1",
    "rehype-raw": "^5.1.0",
    "rehype-react": "^6.0.0",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^7.0.0",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1",
    "ulid": "^2.3.0",
    "unified": "^9.0.0",
    "unist-util-visit": "^2.0.3",
    "use-resize-observer": "^6.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.8",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^16.9.19",
    "@types/react-is": "^17.0.3",
    "@types/styled-components": "^5.1.15",
    "@types/styled-system": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-jsx-to-htm": "^2.0.0",
    "builder": "^4.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "pretty": "^2.0.0",
    "raw-loader": "^4.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  }
}
