{
  "name": "spectacle",
  "version": "5.5.0",
  "description": "ReactJS Powered Presentation Framework",
  "source": "src/index.js",
  "main": "dist/cjs/spectacle.development.js",
  "module": "dist/esm/spectacle.development.js",
  "types": "index.d.ts",
  "scripts": {
    "clean": "rimraf dist lib es",
    "preversion": "yarn run check",
    "version": "yarn clean && yarn build",
    "build-babel": "babel src --ignore \"/__snapshots__/,/**/*.test.js/\"",
    "build-babel-esm": "builder run build-babel -- -d es",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier \"**/*.{js,json,ts,css,md}\"",
    "prettier-fix": "yarn prettier -- --write",
    "rollup": "rollup -c config/rollup.config.js",
    "dev-server": "yarn rollup --watch --open --port=3000",
    "build:dev": "builder run --env \"{\\\"NODE_ENV\\\":\\\"development\\\"}\" rollup --",
    "build:prod": "builder run --env \"{\\\"NODE_ENV\\\":\\\"production\\\"}\" rollup --",
    "start": "builder run dev-server",
    "build": "builder concurrent build:dev build:prod --",
    "deploy": "echo TODO_IMPLEMENT",
    "test": "echo TODO_IMPLEMENT",
    "check": "echo TODO_IMPLEMENT",
    "check-ci": "yarn run check"
  },
  "bin": {
    "spectacle": "./bin/cli.js"
  },
