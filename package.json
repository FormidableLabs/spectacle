{
  "name": "spectacle",
  "version": "6.0.0-alpha.3",
  "description": "ReactJS Powered Presentation Framework",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf dist lib es \"examples/*/dist\"",
    "preversion": "yarn run check",
    "version": "yarn clean && yarn build",
    "build-babel": "babel src --ignore \"/__snapshots__/,/**/*.test.js/\"",
    "build-babel-esm": "builder run build-babel -- -d es",
    "build-babel-cjs": "builder run --env \"{\\\"BABEL_ENV\\\":\\\"cjs\\\"}\" build-babel -- -d lib",
    "build-webpack-base": "webpack",
    "build-webpack-lib-dev": "builder run build-webpack-base -- --config webpack.config.dev.js",
    "build-webpack-lib": "builder run build-webpack-base",
    "build-webpack-examples-js": "builder run build-webpack-base -- --config examples/js/webpack.config.js",
    "build": "builder concurrent --buffer build-babel-esm build-babel-cjs build-webpack-lib build-webpack-lib-dev",
    "build-examples": "builder concurrent --buffer build-webpack-examples-js",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier \"**/*.{js,json,ts,css,md}\"",
    "prettier-check": "yarn prettier --check",
    "prettier-fix": "yarn prettier --write",
    "start:js": "webpack-dev-server --port=3000 --hot --config examples/js/webpack.config.js",
    "test": "jest",
    "check": "yarn lint && yarn prettier-check && yarn test",
    "check-ci": "yarn run check"
  },
  "author": "Formidable Labs <hello@formidable.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle.git"
  },
  "browserslist": "> 0.25%, not dead",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "builder": "^4.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "history": "^4.9.0",
    "marksy": "^8.0.0",
    "normalize-newline": "^3.0.0",
    "prism-react-renderer": "^0.1.7",
    "query-string": "^6.8.2",
    "react-spring": "^8.0.25",
    "styled-components": "^4.3.2",
    "styled-system": "^5.1.1",
    "wonka": "^3.2.0"
  }
}
