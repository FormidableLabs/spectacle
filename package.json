{
  "name": "spectacle",
  "version": "1.0.0-rc1",
  "description": "ReactJS Powered Presentation Framework",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf dist",
    "build:lib": "babel src -d lib",
    "build:dist": "NODE_ENV=production webpack --config webpack.config.production.js",
    "lint": "eslint --ext .js,.jsx src",
    "deploy": "npm run build:dist && surge -p .",
    "preversion": "npm run clean:lib && npm run lint",
    "version": "npm run build:lib",
    "prepublish": "npm run clean:lib && npm run build:lib",
    "start": "node server.js",
    "test": "npm run lint && npm run test-browser",
    "test-browser": "karma start"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "history": "1.13.1",
    "lodash": "3.10.1",
    "mdast": "^2.1.0",
    "mdast-react": "https://github.com/jjt/mdast-react#2ea3bcd",
    "normalize.css": "3.0.3",
    "radium": "0.15.0",
    "react": "^0.14.2",
    "react-addons-transition-group": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "react-tween-state": "0.1.3",
    "redux": "^3.0.4",
    "redux-actions": "^0.8.0",
    "redux-simple-router": "0.0.8",
    "tween-functions": "1.1.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.8.0",
    "eslint-config-defaults": "^7.1.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.6.3",
    "expect": "^1.13.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "node-libs-browser": "^0.5.3",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "surge": "latest",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
