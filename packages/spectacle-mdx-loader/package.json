{
  "name": "spectacle-mdx-loader",
  "version": "0.1.1",
  "description": "Webpack MDX loader for Spectacle presentations",
  "main": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "preversion": "yarn run check",
    "clean": "rimraf \"examples/*/dist\"",
    "build:examples-mdx": "webpack --config ../../examples/mdx/webpack.config.js",
    "build:examples": "yarn build:examples-mdx",
    "start:mdx": "webpack-dev-server --port=4000 --hot --config ../../examples/mdx/webpack.config.js",
    "start": "yarn start:mdx",
    "lint": "eslint .",
    "check": "yarn lint",
    "check:ci": "yarn run check"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle.git"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.3",
    "gray-matter": "^4.0.2",
    "loader-utils": "^1.2.3",
    "normalize-newline": "^3.0.0",
    "spectacle": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@mdx-js/react": "^1.5.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "spectacle": "workspace:*",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
