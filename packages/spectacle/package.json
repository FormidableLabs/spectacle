{
  "name": "spectacle",
  "version": "9.2.1",
  "description": "ReactJS Powered Presentation Framework",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "es/",
    "dist/"
  ],
  "module": "es/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist lib es",
    "preversion": "cd ../../ && pnpm run check",
    "version": "pnpm run clean && pnpm run build",
    "build-babel": "babel src --extensions .tsx,.ts,.jsx,.js --ignore \\\\\\\"/__snapshots__/,**/*.test.*\\\\\\\"",
    "build-babel-esm": "builder run build-babel -- -d es",
    "build-babel-cjs": "builder run --env \"{\\\"BABEL_ENV\\\":\\\"cjs\\\"}\" build-babel -- -d lib",
    "build-webpack-base": "webpack",
    "build-webpack-lib-dev": "builder run build-webpack-base -- --config ./webpack.config.dev.js",
    "build-webpack-lib": "builder run build-webpack-base",
    "build": "builder concurrent --buffer build-babel-esm build-babel-cjs build-webpack-lib build-webpack-lib-dev build-types",
    "build-types": "tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "shx cp ../../README.md ./README.md && shx cp ../../LICENSE ./LICENSE && pnpm run build",
    "postpack": "shx rm ./README.md && shx rm ./LICENSE"
  },
  "author": "Formidable Labs <hello@formidable.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/spectacle.git"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@types/testing-library__jest-dom": "^5.14.5",
    "broadcast-channel": "^3.2.0",
    "broadcastchannel-polyfill": "^1.0.0",
    "dedent": "^0.7.0",
    "history": "^4.9.0",
    "mdast-builder": "^1.1.1",
    "mdast-zone": "^4.0.0",
    "merge-anything": "^3.0.3",
    "mousetrap": "^1.6.5",
    "prism-react-renderer": "^0.1.7",
    "query-string": "^6.8.2",
    "react-fast-compare": "^3.2.0",
    "react-is": "^18.1.0",
    "react-spring": "^8.0.25",
    "react-swipeable": "^6.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^5.1.0",
    "rehype-react": "^6.0.0",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^7.0.0",
    "styled-components": "^4.3.2",
    "styled-system": "5.1.5",
    "ulid": "^2.3.0",
    "unified": "^9.0.0",
    "unist-util-visit": "^2.0.3",
    "use-resize-observer": "^6.1.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.2",
    "@types/mousetrap": "^1.6.8",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-is": "^17.0.3",
    "@types/react-syntax-highlighter": "^15.5.2",
    "@types/styled-components": "^5.1.15",
    "@types/styled-system": "^5.1.5",
    "@types/unist": "^2.0.6",
    "babel-loader": "^8.0.6",
    "builder": "^4.0.0",
    "csstype": "^3.1.0",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "process": "^0.11.10",
